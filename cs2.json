              "name": "Challenges",
              "children": [
                {"name": "Unreliable reporting"},
                {"name": "Synergistic effects"},
                {"name": "Unmasking other causes"},
                {"name": "Short time series data"}
              ]
            }
          ]
        },
        { 
          "name": "Sources of Error",
          "children": [
            {"name": "Model misspecification"},
            {"name": "Incorrect judgement"},
            {"name": "Random variability"},
            {"name": "Errors in data"},
            {"name": "Correlated forecast errors (positive correlation common)"}
          ]
        }
