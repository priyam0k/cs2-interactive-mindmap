        { 
          "name": "Principle of Correspondence",
          "children": [
            {"name": "Consistency between deaths and exposed to risk"}
          ]
        },
        { 
          "name": "Methods of Graduation",
          "children":[
            {"name": "Parametric Formula (Gompertz, Makeham)"},
            {"name": "Standard Table (adjusting existing table)"},
