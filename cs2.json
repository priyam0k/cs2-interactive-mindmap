    {
      "name": "Stochastic Processes",
      "children": [
        {
          "name": "General Concepts",
          "children": [
            { "name": "Increment: Change in value X(t2) - X(t1)" },
            { "name": "Independent Increments" },
            { "name": "Filtration: Expectations conditional on past history" }
          ]
        },
        {
          "name": "Random Walk",
          "children": [
            { "name": "Xn = X0 + Y1 + ... + Yn" },
            { "name": "Yj are IID" },
            { "name": "Discrete/Continuous State Space" },
            {
              "name": "Simple Random Walk",
              "children": [
                { "name": "Yj takes +1 or -1" },
                { "name": "P(Yj=+1) = P(Yj=-1) = 0.5" }
              ]
            },
            {
              "name": "Boundaries",
              "children": [
                { "name": "Reflecting" },
                { "name": "Absorbing" },
                { "name": "Mixed" }
              ]
            }
          ]
        },
        {
          "name": "Markov Chains",
          "children": [
            { "name": "Discrete time, discrete state space" },
            { "name": "Markov Property: Future depends only on current state" },
            { 
              "name": "Chapman-Kolmogorov Equations",
              "children": [
                {"name": "p_i,j(m,n) = Sum_k [p_i,k(m,l) * p_k,j(l,n)]"},
                {"name": "Matrix multiplication (P(m,n) = P(m,l) * P(l,n))"},
                {"name": "Proof not expected in exam"}
              ]
            },
            { 
              "name": "Time-homogeneous Markov Chains",
              "children": [
                {"name": "One-step transition probabilities are time independent"},
                {"name": "Chapman-Kolmogorov simplified"}
              ]
            },
            { 
              "name": "Distribution Fully Determined by",
              "children": [
                {"name": "Initial probability distribution"},
                {"name": "One-step transition probabilities"}
              ]
            },
            { 
              "name": "Stationary Distribution",
              "children": [
                {"name": "p * P = p"},
                {"name": "Sum(pi) = 1"},
                {"name": "Finite state space: At least one stationary distribution"},
                {"name": "Irreducible, aperiodic: Unique stationary distribution"},
                {"name": "Convergence: lim(n->inf) p_i,j(n) = pi_j"}
              ]
            }
          ]
        },
        {
          "name": "Markov Jump Processes",
          "children": [
            { "name": "Continuous time, discrete state space" },
            { "name": "Satisfies Markov property" },
            { "name": "Time-homogeneous if transition probabilities depend only on interval length" },
            { 
              "name": "Inter-event/Holding Times",
              "children": [
                  {"name": "Independent Exponential(lambda) random variables"}
              ]
            },
            { 
              "name": "Generator Matrix (A)",
              "children": [
                {"name": "Transition intensities mu_ij"}
              ]
            },
            { 
              "name": "Kolmogorov Forward Equations",
               "children": [
                {"name": "Differential equations for transition probabilities"}
              ]
            },
            { 
              "name": "Expected Time to Reach State",
              "children": [
                {"name": "Recursive formula"}
              ]
            },
            { "name": "Jump Chains (Embedded Chains): Sequence of states entered" }
          ]
        },
        {
          "name": "Poisson Process",
          "children": [
            { "name": "Used to model counts of events (e.g., claims)" },
            { "name": "Parameter lambda: Average rate of occurrence" },
            { 
              "name": "Properties",
              "children": [
                {"name": "N(t) is Poisson(lambda*t)"},
                {"name": "N(t+s) - N(s) is Poisson(lambda*t)"},
                {"name": "Independent of past"},
                {"name": "Stationary increments"}
              ]
            },
            { "name": "Assumption: At most one claim in short interval" },
            { "name": "Application: Motor insurance claims" }
          ]
        }
      ]
    },
    {
      "name": "Survival Models",
      "children": [
        { 
          "name": "Future Lifetime",
          "children": [
            {"name": "Continuous random variable T_x"},
            {"name": "Distribution Function F_x(t) = P(T_x <= t)"},
            {"name": "Survival Function S_x(t) = P(T_x > t) = 1 - F_x(t)"}
          ]
        },
        { 
          "name": "Actuarial Symbols",
          "children": [
            {"name": "t p_x: Probability of surviving t years from age x"},
            {"name": "t q_x: Probability of dying within t years from age x"},
            {"name": "q_x: Initial rate of mortality (dying in next year)"},
            {"name": "m_x: Central rate of mortality (deaths/person-years)"}
          ]
        },
        { 
          "name": "Force of Mortality (Hazard Rate)",
          "children":[
            {"name": "mu_x: Instantaneous rate of mortality at age x"},
            {"name": "S(t) = exp(-Integral[mu_s ds])"}
          ]
        },
        { 
          "name": "Expected Future Lifetime",
          "children": [
            {"name": "e_x: Curtate expectation (sum p_x^k)"},
            {"name": "e-circle_x: Complete expectation (integral S_x(t) dt)"},
            {"name": "e-circle_x approx e_x + 0.5"}
          ]
        },
        { 
          "name": "Common Mortality Laws",
          "children": [
            {"name": "Gompertz: mu_x = B * c^x"},
            {"name": "Makeham: mu_x = A + B * c^x"}
          ]
        },
        { 
          "name": "Two-State Markov Model (Alive-Dead)",
          "children": [
            {"name": "Assumptions: Markov property, constant mu(x+t), short interval"},
            {"name": "Poisson model of mortality (D ~ Poisson(cE*mu))"}
          ]
        },
        { 
          "name": "Multiple-State Models",
          "children": [
            {"name": "Healthy, Sick, Dead"},
            {"name": "Transitions between states"}
          ]
        },
        { 
          "name": "Censoring",
          "children": [
            {"name": "Incomplete information about durations"},
            {
              "name": "Types",
              "children": [
                {"name": "Right Censoring: Observation cut short"},
                {"name": "Left Censoring: Entry time unknown"},
                {"name": "Interval Censoring: Event within interval (e.g., calendar year of death)"},
                {"name": "Informative Censoring: Censoring event related to outcome"},
                {"name": "Type I Censoring: Fixed termination time"}
              ]
            }
          ]
        },
        { 
          "name": "Non-Parametric Estimation",
          "children": [
            {
              "name": "Kaplan-Meier Estimator",
              "children": [
                {"name": "Product limit estimate of survival function S_hat(t)"},
                {"name": "Hazard zero except at event times"},
                {"name": "d_j / n_j"}
              ]
            },
            {
              "name": "Nelson-Aalen Estimator",
              "children": [
                {"name": "Estimates cumulative hazard function Lambda_hat(t)"},
                {"name": "S_hat(t) = exp(-Lambda_hat(t))"},
                {"name": "Variance formula available"}
              ]
            }
          ]
        },
        { 
          "name": "Proportional Hazards Models",
          "children": [
            {"name": "Effect of covariates on survival"},
            {"name": "Hazard = Baseline_Hazard * exp(Z' * beta)"},
            {"name": "Covariate: Any recorded quantity (age, sex, treatment)"},
            {
              "name": "Cox Model",
              "children":[
                {"name": "Widely used, robust software"},
                {"name": "Partial likelihood estimation (no ties or Breslow approximation)"},
                {"name": "Asymptotic multivariate normal distribution for estimators"},
                {"name": "Asymptotically unbiased"},
                {"name": "Variance matrix = inverse of observed information matrix"}
              ]
            },
            {
              "name": "Model Selection",
              "children":[
                {"name": "Stepwise: Add or eliminate covariates"},
                {"name": "Likelihood ratio statistic"},
                {"name": "Testing for interactions"}
              ]
            }
          ]
        }
      ]
